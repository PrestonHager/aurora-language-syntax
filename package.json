{
  "name": "aurora-language-syntax",
  "displayName": "Aurora Language Syntax",
  "description": "Syntax highlighter for the Aurora programming language.",
  "repository": "https://github.com/PrestonHager/aurora-language-syntax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "aurora",
      "aliases": ["Aurora", "aurora"],
      "extensions": [".aurora"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "aurora",
      "scopeName": "source.aurora",
      "path": "./syntaxes/aurora.tmLanguage.json"
    }]
  }
}
